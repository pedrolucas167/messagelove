{
  "name": "messagelove-backend",
  "version": "1.0.0",
  "description": "Backend para o app Messagelove",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "lint": "eslint .",
    "format": "prettier --write .",
    "render-build": "npm run build",
    "verify-structure": "node -e \"console.log('Estrutura:', require('fs').readdirSync('.'))\""
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.1",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nanoid": "^3.3.11",
    "pg": "^8.16.2",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.2",
    "winston": "^3.17.0",
    "zod": "^3.23.8"
  },
  "engines": {
    "node": ">=20.19.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.16.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
}
